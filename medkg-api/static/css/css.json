[
    {
      "selector": "node",
      "style": {
        "background-color": "#666",
        "text-opacity"	  : "0.5",
        "label"		  	  : "data(display_name)",
        "font-size"    	  : "8px",
        "text-valign"  	  : "center",
        "text-halign"  	  : "center",
        "text-wrap"	 	  : "ellipsis",
        "text-max-width"  : 175
      }
    },
    {
      "selector": "node.left",
      "style": {
        "text-valign"  	  : "center",
        "text-halign"  	  : "left"
      }
    },
    {
      "selector": "node.right",
      "style": {
        "text-valign"  	  : "center",
        "text-halign"  	  : "right"
      }
    },
    {
      "selector": "node.top",
      "style": {
        "text-valign"  	  : "top",
        "text-halign"  	  : "center"
      }
    },
    {
      "selector": "node.bottom",
      "style": {
        "text-valign"  	  : "bottom",
        "text-halign"  	  : "center"
      }
    },
    {
      "selector": "node.center",
      "style": {
        "text-valign"  	  : "center",
        "text-halign"  	  : "center"
      }
    },
    {
      "selector": "node.fontZero",
      "style": {
        "font-size"		  : "0px"
      }
    },
    {
      "selector": "node.fontOne",
      "style": {
        "font-size"		  : "4px"
      }
    },
    {
      "selector": "node.fontTwo",
      "style": {
        "font-size"		  : "7px"
      }
    },
    {
      "selector": "node.fontThree",
      "style": {
        "font-size"		  : "13px"
      }
    },
    {
      "selector": "node.fontFour",
      "style": {
        "font-size"		  : "16px"
      }
    },
    {
      "selector": "node.fontFive",
      "style": {
        "font-size"		  : "20px"
      }
    },
    {
      "selector": "node.fontSix",
      "style": {
        "font-size"		  : "24px"
      }
    },
    {
      "selector": "node.fontSeven",
      "style": {
        "font-size"		  : "27px"
      }
    },
    {
      "selector": "node.fontEight",
      "style": {
        "font-size"		  : "30px"
      }
    },
    {
      "selector": "node.highlightednode",
      "style": {
        "text-opacity"	  : "1",
        "text-wrap"	 	  : "wrap",
        "text-max-width"  : 200
      }
    },
    {
      "selector": "node:selected",
      "style": {
        "text-opacity": "1"
      }
    },
    {
        "selector": "node.highlight",
        "style": {
            "border-color"	  : "#000000",
            "border-width"	  : "2px",
            "font-size"		  : "8px",
            "text-wrap"	 	  : "wrap",
            "text-max-width"  : 250
        }
    },
    {
        "selector": "node.semitransp",
        "style":{ "opacity": "0.5" }
    },
    {
        "selector": "edge.highlight",
        "style": { "width":"3px","opacity":"1","font-size": "6px",
        "mid-target-arrow-color": "#FFF" }
    },
    {
        "selector": "edge.highlightededge",
        "style": { "width":"3px","opacity":"1","font-size": "6px"}
    },
    {
        "selector": "edge.semitransp",
        "style":{ "opacity": "0.2" }
    },
    {
      "selector": "node[Node_Type='Pathway']",
      "style": {
        "shape": "diamond",
        "background-color": "#bcbd22"
      }
    },
    {
      "selector": "node[Node_Type='Biological_process']",
      "style": {
        "shape": "diamond",
        "background-color": "#07779F"
      }
    },
    {
      "selector": "node[Node_Type='Chromosome']",
      "style": {
        "shape": "diamond",
        "background-color": "#17becf"
      }
    },
    {
      "selector": "node[Node_Type='Disease']",
      "style": {
        "shape": "triangle",
        "background-color": "#2ca02c"
      }
    },
    {
      "selector": "node[Node_Type='Gene']",
      "style": {
        "shape": "triangle",
        "background-color": "#e377c2"
      }
    },
    {
      "selector": "node[Node_Type='Protein']",
      "style": {
        "shape": "ellipse",
        "background-color": "#ff7f0e"
      }
    },
    {
      "selector": "node[Node_Type='Transcript']",
      "style": {
        "shape": "ellipse",
        "background-color": "#F8C471"
      }
    },
    {
      "selector": "node[Node_Type='Metabolite']",
      "style": {
        "shape": "pentagon",
        "background-color": "#7f7f7f"
      }
    },
    {
      "selector": "node[Node_Type='Drug']",
      "style": {
        "shape": "hexagon",
        "background-color": "#9467bd",
        "border-width": 2,
        "border-color": "green"
      }
    },
    {
      "selector": "node[Node_Type='Compound']",
      "style": {
        "shape": "hexagon",
        "background-color": "#8c564b",
        "label": "data(id)",
        "border-width": 2,
        "border-color": "violet"
      }
    },
    {
      "selector": "node[Node_Type='Phenotype']",
      "style": {
        "shape": "hexagon",
        "background-color": "white",
        "label": "data(id)",
        "border-width": 2,
        "border-color": "#d62728"	
      }
    },
    {
      "selector": "edge",
      "style": {
        "width": "1px",
        "line-color": "#7f8c8d",
        "curve-style": "unbundled-bezier",
        "opacity": "0.25",
        "label": "data(label)",
        "text-rotation": "autorotate",
        "text-margin-x": "5px",
        "font-size": "0px"
      }
    },
    {
      "selector": "edge.nolabel",
      "style": {
        "label": "",
        "target-arrow-color": "#ccc",
        "target-arrow-shape": "triangle"
      }
    },
    {
      "selector": "edge[Edge_Type='BELONGS_TO_PROTEIN']",
      "style": {
        "line-color": "#078307",
        "target-arrow-color": "#078307",
        "target-arrow-shape": "triangle"
      }
    },
    {
      "selector": "edge[Edge_Type='HAS_SEQUENCE']",
      "style": {
        "line-color": "#5a90d4",
        "target-arrow-color": "#5a90d4",
        "target-arrow-shape": "triangle"
      }
    },
    {
      "selector": "edge[Edge_Type='AN_OFF_LABEL_USE_FOR']",
      "style": {
        "line-color": "#5a90d4",
        "target-arrow-color": "#5a90d4",
        "target-arrow-shape": "triangle"
      }
    },
    {
      "selector": "edge[Edge_Type='IS_AN_INDICATION_FOR']",
      "style": {
        "line-color": "#d03f3f",
        "target-arrow-color": "#d03f3f",
        "target-arrow-shape": "triangle"
      }
    },
    {
      "selector": "edge[Edge_Type='HAS_CARRIER']",
      "style": {
        "line-color": "#d03f3f",
        "target-arrow-color": "#ff9999",
        "target-arrow-shape": "triangle"
      }
    },
    {
      "selector": "edge[Edge_Type='IS_A_CONTRAINDICATION_FOR']",
      "style": {
        "line-color": "#d03f3f",
        "target-arrow-color": "#d03f3f",
        "target-arrow-shape": "triangle"
      }
    },
    {
      "selector": "edge[Edge_Type='IS_A_BIOMARKER_OF']",
      "style": {
        "line-color": "violet",
        "target-arrow-color": "#violet",
        "target-arrow-shape": "triangle"
      }
    },
    {
      "selector": "edge[Edge_Type='ASSOCIATED_WITH']",
      "style": {
        "line-color": "blue",
        "target-arrow-color": "#blue",
        "target-arrow-shape": "triangle"
      }
    },
    {
      "selector": "edge[Edge_Type='HAS_PARENT']",
      "style": {
        "line-color": "#009933",
        "target-arrow-color": "#009933",
        "target-arrow-shape": "triangle"
      }
    },
    {
      "selector": "edge[Edge_Type='HAS_ENZYME']",
      "style": {
        "line-color": "#669999",
        "target-arrow-color": "#669999",
        "target-arrow-shape": "triangle"
      }
    }
    ]
